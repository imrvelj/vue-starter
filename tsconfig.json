{
  "compilerOptions": {
    "module": "esnext",
    "target": "esnext",
    "strict": true,
    "moduleResolution": "node",
    "jsx": "preserve",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true
  },
  "exclude": ["node_modules", "**/__tests__/**/*", "**/__mocks__/**/*"]
}
